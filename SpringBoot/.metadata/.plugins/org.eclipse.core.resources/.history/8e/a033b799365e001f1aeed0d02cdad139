package com.eight;

import org.springframework.context.annotation.ComponentScan;
import org.springframework.context.annotation.Configuration;

@Configuration
@ComponentScan("com.eight")
public class SpringBeanConfigurationFile
{
	public SpringBeanConfigurationFile()
	{
		System.out.println("SpringBeanConfigurationFile constructor entered");
		System.out.println("SpringBeanConfigurationFile constructor exited");
	}
}
