package com.eight;

import org.springframework.context.annotation.AnnotationConfigApplicationContext;

public class test
{
	public static void main(String[] args)
	{
		AnnotationConfigApplicationContext c = new AnnotationConfigApplicationContext(SpringBeanConfigurationFile.class);
		
		controller con = c.getBean(controller.class);
		
		con.save(new emp(1, "Siva", 90000.89));
	}
}
