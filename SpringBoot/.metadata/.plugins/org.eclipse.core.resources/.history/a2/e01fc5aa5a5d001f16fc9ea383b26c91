package com.practice;

import org.springframework.stereotype.Component;

@Component
public class engine
{	
	public engine(int i)
	{
		System.out.println("\nengine default's constructor called\n");
	}
	
	public engine()
	{
		System.out.println("\nengine default's constructor called\n");
	}
	
	public void start()
	{
		System.out.println("Engine started");
	}
}
